{
    "compilerOptions": {
        "noImplicitAny": false,
        "strictNullChecks": false,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "module": "esnext",
        "moduleResolution": "node",
        "target": "es5",
        "sourceMap": true,
        "skipDefaultLibCheck": true,
        "jsx": "preserve",
        //"jsx": "react",
        //"jsxFactory": "h",
        "strict": true,
        "types": [ "webpack-env", "node" ],
        "lib": [ "es2015.promise", "ES6", "es5", "dom" ]
    },
    "exclude": [
        "bin",
        "node_modules",
        "./node_modules",
        "wwwroot",
        "./wwwroot"
    ]
}
